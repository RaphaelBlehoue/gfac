    <script src="{{ asset('bundles/labspagestpl/assets/plugins/nvd3/src/interactiveLayer.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/labspagestpl/assets/plugins/nvd3/src/models/axis.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/labspagestpl/assets/plugins/nvd3/src/models/line.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/labspagestpl/assets/plugins/nvd3/src/models/lineWithFocusChart.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/labspagestpl/assets/plugins/mapplic/js/hammer.js') }}" type="text/javascript"></script>
