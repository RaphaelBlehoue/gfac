<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <title>{% block title %}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <link rel="apple-touch-icon" href="{{ asset('bundles/labspagestpl/pages/ico/60.png') }}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/labspagestpl/pages/ico/76.png') }}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/labspagestpl/pages/ico/120.png') }}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/labspagestpl/pages/ico/152.png') }}">
    <link rel="icon" type="image/x-icon" href="favicon.ico" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/pace/pace-theme-flash.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/boostrapv3/css/bootstrap.min.css') }} " rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/font-awesome/css/font-awesome.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/jquery-scrollbar/jquery.scrollbar.css') }}" rel="stylesheet" type="text/css" media="screen" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/bootstrap-select2/select2.css') }}" rel="stylesheet" type="text/css" media="screen" />
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/switchery/css/switchery.min.css') }}" rel="stylesheet" type="text/css" media="screen" />
    {% block stylesheets %} {% endblock %}
    <link href="{{ asset('bundles/labspagestpl/pages/css/pages-icons.css') }}" rel="stylesheet" type="text/css">
    <link class="main-stylesheet" href="{{ asset('bundles/labspagestpl/pages/css/pages.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/labspagestpl/assets/css/style.css') }}" rel="stylesheet" type="text/css" />
    <!--[if lte IE 9]>
    <link href="{{ asset('bundles/labspagestpl/pages/css/ie9.css') }}" rel="stylesheet" type="text/css" />
    <![endif]-->
    <!--[if lt IE 9]>
    <link href="{{ asset('bundles/labspagestpl/assets/plugins/mapplic/css/mapplic-ie.css') }}" rel="stylesheet" type="text/css" />
    <![endif]-->
    <script type="text/javascript">
        window.onload = function()
        {
            // fix for windows 8
            if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
                document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="{{ asset('bundles/labspagestpl/pages/css/windows.chrome.fix.css') }}" />'
        }
    </script>
</head>
<body class="fixed-header  dashboard ">

{% include 'LabsPagesTplBundle:Includes:sidebar.html.twig' %}

<!-- START PAGE-CONTAINER -->
<div class="page-container">
    <!-- START HEADER -->
    {% include 'LabsPagesTplBundle:Includes:header.html.twig' %}
    <!-- END HEADER -->

    <!-- START PAGE CONTENT WRAPPER -->
    <div class="page-content-wrapper">
        <!-- START PAGE CONTENT -->
        <div class="content sm-gutter">
                {% block body %}{% endblock %}
        </div>
        <!-- END PAGE CONTENT -->


        <!-- START COPYRIGHT -->

        <!-- START CONTAINER FLUID -->
        <div class="container-fluid container-fixed-lg footer">
            <div class="copyright sm-text-center">
                <p class="small no-margin pull-left sm-pull-reset">
                    <span class="hint-text">Copyright ©  </span>
                    <span class="font-montserrat">Facturation</span>.
                    <span class="hint-text">All rights reserved. </span>
                    <span class="sm-block"><a href="#" class="m-l-10 m-r-10">Terms of use</a> | <a href="#" class="m-l-10">Privacy Policy</a></span>
                </p>
                <p class="small no-margin pull-right sm-pull-reset">
                    <a href="#">Hand-crafted</a> <span class="hint-text">&amp; Made with Love ®</span>
                </p>
                <div class="clearfix"></div>
            </div>
        </div>
        <!-- END COPYRIGHT -->
    </div>
    <!-- END PAGE CONTENT WRAPPER -->
</div>
<!-- END PAGE CONTAINER -->

<!-- BEGIN VENDOR JS -->
<script src="{{ asset('bundles/labspagestpl/assets/plugins/pace/pace.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery/jquery-1.11.1.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/modernizr.custom.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-ui/jquery-ui.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/boostrapv3/js/bootstrap.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery/jquery-easy.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-unveil/jquery.unveil.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-bez/jquery.bez.min.js') }}"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-ios-list/jquery.ioslist.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-actual/jquery.actual.min.js') }}"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/labspagestpl/assets/plugins/bootstrap-select2/select2.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/labspagestpl/assets/plugins/classie/classie.js') }}"></script>
<script src="{{ asset('bundles/labspagestpl/assets/plugins/switchery/js/switchery.min.js') }}" type="text/javascript"></script>
{% block javascript %}{% endblock %}


<!-- END VENDOR JS -->
<!-- BEGIN CORE TEMPLATE JS -->
<script src="{{ asset('bundles/labspagestpl/pages/js/pages.min.js') }}"></script>
<!-- END CORE TEMPLATE JS -->
<!-- BEGIN PAGE LEVEL JS -->
{% block dashboard %}{% endblock %}
{% block otherscript %}{% endblock %}
<script src="{{ asset('bundles/labspagestpl/assets/js/scripts.js') }}" type="text/javascript"></script>
<!-- END PAGE LEVEL JS -->
</body>
</html>